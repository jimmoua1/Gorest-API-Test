{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "http://example.com/example.json",
  "type": "object",
  "title": "The root schema",
  "description": "The root schema comprises the entire JSON document.",
  "default": {},
  "examples": [
    {
      "meta": null,
      "data": {
        "gender": "Male",
        "name": "Alan",
        "email": "test0000000000000@email.com",
        "status": "active"
      }
    }
  ],
  "required": [
    "meta",
    "data"
  ],
  "properties": {
    "meta": {
      "$id": "#/properties/meta",
      "type": "null",
      "title": "The meta schema",
      "description": "An explanation about the purpose of this instance.",
      "default": null,
      "examples": [
        null
      ]
    },
    "data": {
      "$id": "#/properties/data",
      "type": "object",
      "title": "The data schema",
      "description": "An explanation about the purpose of this instance.",
      "default": {},
      "examples": [
        {
          "gender": "Male",
          "name": "Alan",
          "email": "test0000000000000@email.com",
          "status": "active"
        }
      ],
      "required": [
        "gender",
        "name",
        "email",
        "status"
      ],
      "properties": {
        "gender": {
          "$id": "#/properties/data/properties/gender",
          "type": "string",
          "title": "The gender schema",
          "description": "An explanation about the purpose of this instance.",
          "default": "",
          "examples": [
            "Male"
          ]
        },
        "name": {
          "$id": "#/properties/data/properties/name",
          "type": "string",
          "title": "The name schema",
          "description": "An explanation about the purpose of this instance.",
          "default": "",
          "examples": [
            "Alan"
          ]
        },
        "email": {
          "$id": "#/properties/data/properties/email",
          "type": "string",
          "title": "The email schema",
          "description": "An explanation about the purpose of this instance.",
          "default": "",
          "examples": [
            "test0000000000000@email.com"
          ]
        },
        "status": {
          "$id": "#/properties/data/properties/status",
          "type": "string",
          "title": "The status schema",
          "description": "An explanation about the purpose of this instance.",
          "default": "",
          "examples": [
            "active"
          ]
        }
      },
      "additionalProperties": true
    }
  },
  "additionalProperties": true
}